{
  "name": "ranobenet",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:typed-css-modules": "tcm -w -p '**/*.module.css'",
    "dev:path": "pathpida --ignorePath .gitignore -o modules/utils/ --watch",
    "build": "pathpida --ignorePath .gitignore -o modules/utils/ && tcm -p '**/*.module.css' && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "openapi-gen": "run-s openapi-gen:prepare openapi-gen:build",
    "openapi-gen:prepare": "npx rimraf ranobe-net-api",
    "openapi-gen:build": "npx openapi2aspida"
  },
  "dependencies": {
    "@aspida/fetch": "^1.7.1",
    "@aspida/react-query": "^1.8.0",
    "@hookform/devtools": "^4.0.2",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.8.8",
    "@mdx-js/loader": "^2.0.0",
    "@next/mdx": "^12.1.0",
    "@tailwindcss/forms": "^0.5.0",
    "class-validator": "^0.13.2",
    "clsx": "^1.1.1",
    "firebase": "^9.6.6",
    "jotai": "^1.6.0",
    "next": "12.0.10",
    "next-pwa": "^5.4.4",
    "next-themes": "^0.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.26.0",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.16",
    "react-toastify": "^8.2.0",
    "react-use": "^17.3.2",
    "tailwindcss": "^3.0.23",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/node": "17.0.15",
    "@types/react": "17.0.39",
    "autoprefixer": "^10.4.2",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.17.0",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.3.1",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "ts-node": "^10.5.0",
    "typed-css-modules": "^0.7.0",
    "typescript": "4.5.5"
  },
  "browserslist": [
    "last 1 version"
  ]
}
