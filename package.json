{
  "name": "ranobenet",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:typed-css-modules": "tcm -w -p '**/*.module.css'",
    "dev:path": "pathpida --ignorePath .gitignore -o modules/utils/ --watch",
    "build": "pathpida --ignorePath .gitignore -o modules/utils/ && tcm -p '**/*.module.css' && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "openapi-gen": "run-s openapi-gen:prepare openapi-gen:build",
    "openapi-gen:prepare": "npx rimraf ranobe-net-api",
    "openapi-gen:build": "npx openapi2aspida"
  },
